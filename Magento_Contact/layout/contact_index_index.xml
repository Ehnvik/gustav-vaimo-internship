        <referenceContainer name="content">
            <block name="custom.function.mixin" template="Magento_Theme::functions/custom-function-two-mixin.phtml" before="-" />
            <block name="custom.object.mixin.two" template="Magento_Theme::objects/custom-object-two-mixin.phtml" before="-" />
            <block name="custom.widget.mixin.two" template="Magento_Theme::widgets/custom-widget-two-mixin.phtml" before="-" />
