<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>

        <!-- Move -->
        <move element="custom.wishlist" destination="custom.header.container" after="my-account-link" />

        <referenceContainer name="header-wrapper">
            <referenceContainer name="custom.header.container">
                <block name="custom.wishlist" template="Magento_Wishlist::custom-wishlist.phtml" />
            </referenceContainer>
        </referenceContainer>
    </body>
</page>
